{
  "name": "tekshila-frontend",
  "version": "2.0.0",
  "description": "Modern web frontend for Tekshila - AI-powered code documentation tool",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "vite preview --port 3000",
    "api": "python api_bridge.py",
    "start": "concurrently \"npm run api\" \"npm run dev\"",
    "start:prod": "concurrently \"npm run api\" \"npm run serve\""
  },
  "keywords": [
    "ai",
    "documentation",
    "code-analysis",
    "github",
    "frontend"
  ],
  "author": "Tekshila Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^9.2.0",
    "vite": "^7.0.4"
  }
}
