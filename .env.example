# GitHub OAuth Configuration (Required)
GITHUB_CLIENT_ID=your_github_client_id_here
GITHUB_CLIENT_SECRET=your_github_client_secret_here

# JWT Secret for Session Management (Required)
JWT_SECRET=your_jwt_secret_here_min_32_chars

# Google Gemini API Configuration (Required for actual AI generation)
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_API_URL=https://generativelanguage.googleapis.com/v1beta/models/gemini-pro:generateContent

# Optional: OpenAI API Configuration (Alternative to Gemini)
OPENAI_API_KEY=your_openai_api_key_here

# Application URL (Automatically set by Vercel)
# VERCEL_URL=your-app.vercel.app